{
  "name": "react-bulma-components",
  "version": "3.0.1-3",
  "description": "React components for Bulma framework",
  "main": "dist",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "eslint . && jest --coverage",
    "eslint": "eslint .",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "clean:build": "rm -rf dist && rm -rf lib && rm -rf full",
    "build-storybook-static": "build-storybook -c .storybook -o .out",
    "prepare": "yarn clean:build && npm run build && yarn clean:test",
    "deploy-storybook": "storybook-to-ghpages",
    "build": "WEBPACK_ENV=INCLUDE_CSS webpack --progress --color --mode=production && webpack --progress --color --mode=production && yarn create:lib",
    "create:lib": "babel ./src --out-dir ./lib --copy-files --source-maps",
    "clean:test": "rm -rf $(find ./lib -name \"__test__\")"
  },
  "storybook-deployer": {
    "gitUsername": "Couds",
    "gitEmail": "john.elric@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/couds/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/couds/react-bulma-components/issues"
  },
  "homepage": "https://github.com/couds/react-bulma-components#readme",
  "dependencies": {
    "bulma": "0.7.3"
  },
  "peerDependencies": {
    "prop-types": ">=15.6.0",
    "react": "16.3.0",
    "react-dom": "16.3.0"
  },
