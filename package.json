{
  "name": "react-bulma-components",
  "version": "4.0.7",
  "description": "React components for Bulma framework",
  "main": "./cjs/index.js",
  "types": "./src/index.d.ts",
  "sideEffects": false,
  "files": [
    "esm",
    "cjs",
    "index.esm.mjs",
    "**/*.d.ts"
  ],
  "directories": {
    "esm": "esm",
    "cjs": "cjs"
  },
  "scripts": {
    "test": "jest",
    "test:cov": "eslint ./src && jest --coverage",
    "eslint": "eslint ./src",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "docs": "cd documentation && npm start",
    "clean:build": "rm -rf esm cjs",
    "build-storybook-static": "build-storybook -c .storybook -o .out",
    "prepublish": "npm run clean:build && npm run build-all",
    "deploy-storybook": "storybook-to-ghpages",
    "babel": "babel ./src --source-maps --ignore \"**/__test__,**/*.story.js\"",
    "build-commonjs": "TARGET=commonjs npm run babel -- --out-dir ./cjs ",
    "build-esm": "npm run babel -- --out-dir ./esm",
    "build-all": "npm run build-commonjs && npm run build-esm",
    "upgrade-dependencies": "npx npm-check --update"
  },
  "storybook-deployer": {
    "gitUsername": "Couds",
    "gitEmail": "john.elric@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/couds/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout",
    "responsive"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/couds/react-bulma-components/issues"
  },
  "homepage": "https://react-bulma.dev",
  "dependencies": {
    "classnames": "2.3.1"
  },
  "peerDependencies": {
    "bulma": ">=0.9.2",
    "prop-types": ">=15.6.0",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.13.16",
    "@babel/plugin-transform-react-constant-elements": "7.13.13",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-events": "6.2.9",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storysource": "6.2.9",
    "@storybook/addon-viewport": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/cli": "6.2.9",
    "@storybook/components": "6.2.9",
    "@storybook/core-events": "6.2.9",
    "@storybook/node-logger": "6.2.9",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.2.9",
    "@storybook/storybook-deployer": "2.8.7",
    "@storybook/theming": "6.2.9",
    "@types/react": "17.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-runtime": "6.26.0",
    "bulma": "0.9.2",
    "chai": "4.3.4",
    "coveralls": "3.1.0",
    "emotion": "11.0.0",
    "emotion-theming": "11.0.0",
    "enzyme": "3.11.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "jsdom": "16.5.3",
    "moment": "2.29.1",
    "node-sass": "5.0.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "sass": "1.32.11",
    "sass-loader": "10.1.1",
    "source-map-support": "0.5.19",
    "storybook-addon-react-docgen": "1.2.42",
    "storybook-addon-themes": "6.1.0",
    "typescript": "4.2.4",
    "webpack": "4.46.0"
  }
}
