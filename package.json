{
  "name": "react-bulma-components",
  "version": "4.0.0-RC.0",
  "description": "React components for Bulma framework",
  "main": "./lib/index.js",
  "types": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "test:cov": "eslint . && jest --coverage",
    "eslint": "eslint .",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "docs": "cd documentation && npm start",
    "clean:build": "rm -rf lib",
    "build-storybook-static": "build-storybook -c .storybook -o .out",
    "prepare": "npm run clean:build && npm run build && npm run clean:test",
    "deploy-storybook": "storybook-to-ghpages",
    "build": "babel ./src --out-dir ./lib --copy-files --source-maps",
    "clean:test": "rm -rf $(find ./lib -name \"__test__\")",
    "upgrade-dependencies": "npx npm-check --update"
  },
  "storybook-deployer": {
    "gitUsername": "Couds",
    "gitEmail": "john.elric@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/couds/react-bulma-components.git"
  },
  "keywords": [
    "react",
    "bulma",
    "bulma.io",
    "css",
    "bootstrap",
    "modal",
    "layout",
    "responsive"
  ],
  "author": "John Benavides",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/couds/react-bulma-components/issues"
  },
  "homepage": "https://github.com/couds/react-bulma-components#readme",
  "dependencies": {
    "classnames": "2.2.6"
  },
