    "@storybook/addon-options": "3.2.17",
    "@storybook/cli": "3.2.16",
    "@storybook/react": "3.2.16",
    "@storybook/storybook-deployer": "2.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
