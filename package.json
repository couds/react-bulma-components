    "file-loader": "1.1.11",
    "jest": "22.4.3",
    "jsdom": "11.9.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.3",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
